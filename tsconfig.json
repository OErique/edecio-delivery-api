{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strict": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": false
  },
  "include": ["src"]
}
